{
    "scripts": {
        "script": {
            "class": "Script",
            "properties": {
                "required": [
                    "id", "x", "y", "width", "height", "type"
                ],
                "optional": [
                    { "name": "is_active", "default": "false" },
                    { "name": "delay", "default": "-1" }
                ]
            },
            "children": {
                "rotate_entity": {
                    "class": "RotateEntityScript",
                    "properties": {
                        "required": [
                            "target", "angular_speed"
                        ],
                        "optional": [
                            { "name": "degrees", "default": "null" }
                        ]
                    }
                }
            }
        }
    }
}